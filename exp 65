import java.util.*;
 public class Main
 {
 public static void main(String[] args)
    {
        int x,y,z,i;
        Scanner in = new Scanner(System.in);
        System.out.print("Enter the first number : ");
        x = in.nextInt();  
		System.out.print("Enter the second number: ");
		y = in.nextInt(); 
		z = x / y;
		i= x - (z * y);
		System.out.println("\nThe remainder is : " +i); 
	}
 }

